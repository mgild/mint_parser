[package]
name = "mint_parser"
version = "0.1.0"
edition = "2021"

[dependencies]
solana-client = "1.18.25"
solana-program = "1.18.25"
solana-account-decoder = "1.18.25"
solana-sdk = "1.18.25"
anchor-client = "0.30.0"
spl-token = "4.0.0"
anyhow = "1.0.75"
tokio = { version = "1.36.0", features = ["full"] }
reqwest = { version = "0.11.24", features = ["json"] }
serde = { version = "1.0.197", features = ["derive"] }
serde_json = "1.0.114"
clap = { version = "4.5.1", features = ["derive"] }
bincode = "2.0.1"
lazy_static = "1.5.0"
lifinity_v2_sdk = { path = "third_party/lifinity_sdk/lifinity-v2-sdk" }
